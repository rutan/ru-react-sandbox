{
  "name": "ru-react-sandbox",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite preview",
    "dev": "vite",
    "build": "run-s lint clean build:*",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --check \"./src/**/*.{html,js,jsx,ts,tsx,json,css,scss}\"",
    "lint:tsc": "tsc --noEmit",
    "format": "run-s format:*",
    "format:prettier": "prettier --write \"./**/*.{html,js,jsx,ts,tsx,json,css,scss}\"",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vite": "^5.1.8"
  }
}
